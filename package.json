{
  "name": "voxxy-mobile",
  "license": "0BSD",
  "version": "1.1.8",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:dev": "node scripts/set-env.js development && expo start",
    "start:staging": "node scripts/set-env.js staging && expo start",
    "start:prod": "node scripts/set-env.js production && expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:ios": "node scripts/set-env.js production && eas build --platform ios",
    "build:android": "node scripts/set-env.js production && eas build --platform android",
    "logs:clean": "node scripts/replace-console-logs.js",
    "test:logs": "node scripts/set-env.js development && expo start -c",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "prebuild": "npm run check"
  },
  "dependencies": {
    "@expo-google-fonts/montserrat": "^0.3.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/native": "^7.1.4",
    "@react-navigation/native-stack": "^7.3.8",
    "expo": "~53.0.0",
    "expo-clipboard": "^7.1.5",
    "expo-constants": "~17.1.7",
    "expo-contacts": "~14.2.5",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.4",
    "expo-status-bar": "~2.2.3",
    "lucide-react-native": "^0.511.0",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-feather": "^1.1.2",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-vector-icons": "^10.2.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/eslint-parser": "^7.28.0",
    "@react-native-community/cli": "latest",
    "@types/react": "~19.0.10",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "react-native-svg-transformer": "^1.5.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
